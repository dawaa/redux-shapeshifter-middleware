    "eslint-plugin-import": "^2.18.2",
    "i": "^0.3.6",
    "mocha": "^8.2.0",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "sinon": "^9.2.0",
    "sinon-chai": "^3.5.0"
