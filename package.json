{
  "name": "redux-shapeshifter-middleware",
  "version": "0.10.1-rc1",
  "description": "Redux middleware for empowering your actions using `axios` and `qs` libraries combined.",
  "main": "./lib/middleware.js",
  "author": "Alejandro Andersson <alejandro.andersson@hyperisland.se>",
  "repository": {
    "type": "git",
    "url": "https://github.com/dawaa/redux-shapeshifter-middleware.git"
  },
  "homepage": "https://github.com/dawaa/redux-shapeshifter-middleware",
  "bugs": {
    "url": "https://github.com/dawaa/redux-shapeshifter-middleware/issues"
  },
  "license": "MIT",
  "scripts": {
    "prepublish": "yarn run test && yarn run build:babel",
    "test": "cross-env BABEL_ENV=commonjs nyc mocha --require babel-polyfill --require babel-core/register --reporter spec test/*.js",
    "test:debug": "mocha --inspect --require babel-polyfill --require babel-core/register test/*.js",
    "test:watch": "yarn run test --watch",
    "build:babel": "rimraf ./lib && babel src --out-dir lib --copy-files"
  },
  "keywords": [
    "redux",
    "shapeshifter",
    "middleware",
    "redux-middleware",
    "action",
    "actions",
    "redux-store",
    "generator-action",
    "generator-redux",
    "generator",
    "axios-action",
    "ajax-action",
    "http-action",
    "ajax",
    "http"
  ],
  "dependencies": {
    "axios": "^0.17.0",
    "babel-runtime": "^6.26.0"
  },
  "peerDependencies": {
    "axios": ">= 0.17"
  },
  "devDependencies": {
    "axios": "^0.17.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "cross-env": "^5.1.0",
    "decache": "^4.4.0",
    "i": "^0.3.6",
    "mocha": "^4.0.1",
    "npm": "^5.5.1",
    "nyc": "^11.3.0",
    "rimraf": "^2.6.2",
    "sinon": "^4.0.2",
    "sinon-chai": "^3.0.0"
  }
}
