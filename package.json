    "ajax",
    "http"
  ],
  "dependencies": {
    "@babel/runtime": "^7.12.1",
    "axios": "^0.21.0"
  },
  "peerDependencies": {
    "axios": ">= 0.21.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-transform-destructuring": "^7.12.1",
